<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <nav class='nav navbar-nav navbar-left' >
      <span>ECサイト</span>
    </nav>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <% if end_user_signed_in? %>  <!-- ユーザーが現在サインインをしているか？　-->
          <li><%= link_to ' Mypage', end_user_path(current_end_user),class: 'glyphicon glyphicon-home' %></li>
          <li>Cart</li> <!--カート一覧-->
          <li><%= link_to ' logout', destroy_end_user_session_path, method: :delete,class: 'glyphicon glyphicon-log-out logout' %></li>
        <% elsif admin_signed_in? %>
          <li>Order</li> <!--受注一覧-->
          <li><%= link_to ' 会員一覧', admin_end_users_path,class: 'glyphicon glyphicon-user' %></li> 
          <li><%= link_to ' logout', destroy_admin_session_path, method: :delete,class: 'glyphicon glyphicon-log-out logout' %></li>
        <% else %>
          <li>ユーザー</li>
          <li><%= link_to ' sign up', new_end_user_registration_path,class: 'glyphicon glyphicon-edit sign_up' %></li>
          <li><li><%= link_to ' login', new_end_user_session_path ,class: 'glyphicon glyphicon-log-in sign_in'%></li></li> <!--エンドユーザーログインへのリンク-->
          <li>管理者</li>
          <li><li><%= link_to ' login', new_admin_session_path ,class: 'glyphicon glyphicon-log-in sign_in'%></li></li> <!--エンドユーザーログインへのリンク-->
        <% end %>
      </ul>
    </nav>
  </div>
</header>

