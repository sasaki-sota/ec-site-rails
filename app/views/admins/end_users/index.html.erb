<h3><strong>会員一覧画面</strong></h3>

<table class="table">
  <thead>
    <tr>
      <th>顧客id</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>

  <tbody>
  <%# 現状のままだと退会したユーザーは表示されなくなっている %>
    <% @end_users.each do |end_user| %> 
      <tr>
        <td><%= end_user.id %></td>
        <td><%= end_user.last_name %> <%= end_user.first_name %></td>
        <td><%= end_user.email %></td>
        <td><% if end_user.id == EndUser.with_deleted %>
          退会済み
        <% end %></td>
      </tr>
    <% end %>
  </tbody>
</table>